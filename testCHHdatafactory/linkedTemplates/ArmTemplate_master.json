{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "testCHHdatafactory"
		},
		"Salesforce_pcpy_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Salesforce_pcpy'"
		},
		"Salesforce_pcpy_securityToken": {
			"type": "secureString",
			"metadata": "Secure string for 'securityToken' of 'Salesforce_pcpy'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"Salesforce_pcpy_properties_typeProperties_environmentUrl": {
			"type": "string",
			"defaultValue": "https://chytryhonza--partialcpy.my.salesforce.com/"
		},
		"Salesforce_pcpy_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "zdenek.coufal@chytryhonza.cz.partialcpy"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "testCHHdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Salesforce_pcpy_password": {
						"value": "[parameters('Salesforce_pcpy_password')]"
					},
					"Salesforce_pcpy_securityToken": {
						"value": "[parameters('Salesforce_pcpy_securityToken')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"Salesforce_pcpy_properties_typeProperties_environmentUrl": {
						"value": "[parameters('Salesforce_pcpy_properties_typeProperties_environmentUrl')]"
					},
					"Salesforce_pcpy_properties_typeProperties_username": {
						"value": "[parameters('Salesforce_pcpy_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}